<tbody>
  <tr>
      <% if user_signed_in? %>

        <td><%= link_to('Logout', destroy_user_session_path, :method => :delete) %></td>

      <% else %>

        <td><%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %> </td>
        <td><%= link_to "Sign in", user_session_path, method: 'new' %> </td>
        <td><%= link_to "Sign up", user_registration_path, method: 'create' %></td>

      <% end %>
  </tr>
</tbody>
