<!DOCTYPE html>
<html>
  <head>
    <title>NewsmapWebapp</title>
    <%= csrf_meta_tags %>
    <%= javascript_tag "var AUTH_TOKEN = '#{form_authenticity_token}';" if protect_against_forgery? %>

    <%= javascript_include_tag "application" %>
    <%= stylesheet_link_tag "application" %>
    <link href="https://fonts.googleapis.com/css?family=Dosis:200,300,400,500,600" rel="stylesheet">  </head>
  <body>
    <div class="container-fluid ">
      <%= render 'layouts/navbar' %></li>
      <!-- <%= render 'layouts/log' %> -->
      <%= render 'layouts/messages' %>

      <% if current_user != nil %>

        <div class = "index-container" data-id="<%=current_user.id%>"></div>

      <% end %>

      <%= yield %>
    </div>

  </body>
</html>
